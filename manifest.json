{
    "manifest_version": 3,
    "name": "WordleBot",
    "version": "1.0.0",
    "description": "Shows you WordleBot",
    "action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "js": [
                "script.js"
            ],
            "matches": [
                "https://www.nytimes.com/interactive/2022/upshot/wordle-bot.html"
            ]
        }
    ]
}